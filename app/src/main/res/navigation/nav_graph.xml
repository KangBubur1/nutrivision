<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/homeFragment">


    <fragment
        android:id="@+id/detailFood"
        android:name="com.example.nutrivision.ui.detail.DetailFoodFragment"
        android:label="fragment_detail_food"
        tools:layout="@layout/fragment_detail_food" />
    <fragment
        android:id="@+id/homeFragment"
        android:name="com.example.nutrivision.ui.home.HomeFragment"
        android:label="HomeFragment" />
    <fragment
        android:id="@+id/cameraFragment"
        android:name="com.example.nutrivision.ui.camera.CameraFragment"
        android:label="fragment_camera"
        tools:layout="@layout/fragment_camera" />
    <fragment
        android:id="@+id/usersettingFragment"
        android:name="com.example.nutrivision.ui.settings.UserSettingFragment"
        android:label="user_setting"
        tools:layout="@layout/user_setting" />
    <action
        android:id="@+id/action_userSettingFragment_to_profileFragment"
        app:destination="@id/profileFragment" />
    <fragment
        android:id="@+id/profileFragment"
        android:name="com.example.nutrivision.ui.profile.ProfileFragment"
        android:label="ProfileFragment"
        tools:layout="@layout/fragment_profile"/>

</navigation>